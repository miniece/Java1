<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="499.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <TextField fx:id="gameText" prefHeight="70.0" prefWidth="464.0" />
   <Button fx:id="North" mnemonicParsing="false" onAction="#moveNorth" text="North" />
   <Button fx:id="east" mnemonicParsing="false" onAction="#moveEast" text="East" />
   <Button fx:id="west" mnemonicParsing="false" onAction="#moveWest" text="West" />
   <Button fx:id="South" mnemonicParsing="false" onAction="#moveSouth" text="South" />
   <Button fx:id="Search" mnemonicParsing="false" onAction="#searchRoom" text="Search" />
   <Button fx:id="Sleep" mnemonicParsing="false" onAction="#sleep" text="Sleep" />
   <Button fx:id="run" mnemonicParsing="false" onAction="#run" text="Run" />
   <Button fx:id="Fight" mnemonicParsing="false" onAction="#fight" text="Fight" />
   <FlowPane prefHeight="22.0" prefWidth="464.0">
      <children>
         <Label fx:id="PlayerAttributes" prefHeight="18.0" prefWidth="464.0" text="Player Attributes" textAlignment="CENTER" />
         <Label fx:id="monsterAttributes" layoutX="10.0" layoutY="10.0" prefHeight="18.0" prefWidth="464.0" text="Monster Attributes" textAlignment="CENTER" />
      </children></FlowPane>
</VBox>
